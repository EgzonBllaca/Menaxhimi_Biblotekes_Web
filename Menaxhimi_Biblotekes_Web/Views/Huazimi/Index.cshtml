                    @Html.DisplayFor(modelItem => item.CreatedByUserID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedOn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastUpdatedByUserID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastUpdatedOn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Libri.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pjesemarresi.Id)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
