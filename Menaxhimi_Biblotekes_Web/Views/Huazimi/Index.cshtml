@model IEnumerable<Menaxhimi_Biblotekes.Models.Huazimi>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">

    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataHuazimit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AfatiKthimit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataKthimit)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Verejtje)
